<?xml version='1.0' encoding='utf-8'?>
<widget id="vn.edu.greenwich.rentalz" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>RentalZ</name>
    <description>
        The app will allow the user to find, add and edit rental listings. The app will be called RentalZ.
    </description>
    <author email="khoapdgcs18003@fpt.edu.vn" href="http://cordova.io">
        Chuck Norris
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
